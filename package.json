{
  "name": "angular-pwa-ngrx-formly-routerstate",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --proxy-config proxy.conf.js",
    "start:hmr": "npm run start -- --hmr",
    "build": "ng build --prod && npm run transloco:optimize",
    "lint": "ng lint && stylelint \"**/*.scss\" --syntax scss && htmlhint \"src/app/**/*.html\" --config .htmlhintrc",
    "test": "ng test",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/angular-pwa-ngrx-formly-routerstate/stats.json",
    "transloco:optimize": "transloco-optimize dist/angular-pwa-ngrx-formly-routerstate/assets/i18n",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.8",
    "@angular/common": "~11.0.8",
    "@angular/compiler": "~11.0.8",
    "@angular/core": "~11.0.8",
    "@angular/forms": "~11.0.8",
    "@angular/platform-browser": "~11.0.8",
    "@angular/platform-browser-dynamic": "~11.0.8",
    "@angular/router": "~11.0.8",
    "@angular/service-worker": "~11.0.8",
    "@ngneat/content-loader": "^6.0.0",
    "@ngneat/transloco": "^2.20.0",
    "@ngrx/component": "^11.0.0-beta.0",
    "@ngrx/effects": "^11.0.0-beta.0",
    "@ngrx/entity": "^11.0.0-beta.0",
    "@ngrx/router-store": "^11.0.0-beta.0",
    "@ngrx/store": "^11.0.0-beta.0",
    "@ngx-formly/core": "^5.10.11",
    "@reduxjs/toolkit": "^1.5.0",
    "ngrx-store-localstorage": "^10.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/architect": "~0.1100.6",
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular-devkit/core": "^11.0.6",
    "@angular-devkit/schematics": "^11.0.6",
    "@angular-eslint/builder": "^1.1.0",
    "@angular-eslint/eslint-plugin": "^1.1.0",
    "@angular-eslint/eslint-plugin-template": "^1.1.0",
    "@angular-eslint/template-parser": "^1.1.0",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "~11.0.8",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@componizer/schematics": "0.0.6",
    "@ngneat/tailwind": "^5.1.0",
    "@ngneat/transloco-optimize": "^1.0.2",
    "@ngneat/transloco-validator": "^1.0.1",
    "@ngrx/schematics": "^11.0.0-beta.0",
    "@ngrx/store-devtools": "^11.0.0-beta.0",
    "@tailwindcss/forms": "^0.2.1",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ngrx": "^1.13.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-rxjs": "^2.1.7",
    "htmlhint": "^0.14.2",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.3.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "ng-packagr": "^11.0.3",
    "postcss": "8.2.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "tailwindcss": "2.0.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,html}": [
      "eslint --fix"
    ],
    "src/**/*.{scss,css}": [
      "stylelint --syntax scss --fix"
    ],
    "src/**/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
