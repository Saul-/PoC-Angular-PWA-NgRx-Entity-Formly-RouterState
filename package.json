{
  "name": "angular-pwa-ngrx-formly-routerstate",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --proxy-config proxy.conf.js",
    "start:hmr": "npm run start -- --hmr",
    "build": "ng build --prod && npm run transloco:optimize",
    "deploy": "ng deploy --base-href=/poc-angular-pwa-ngrx-entity-formly-routerstate/",
    "deploy:standalone": "ng build --prod --base-href \"/poc-angular-pwa-ngrx-entity-formly-routerstate/\" && npm run transloco:optimize && npx angular-cli-ghpages --dir=dist/angular-pwa-ngrx-formly-routerstate",
    "test": "ng test",
    "lint": "ng lint && stylelint \"**/*.scss\" --syntax scss && htmlhint \"src/app/**/*.html\" --config .htmlhintrc",
    "e2e": "ng e2e",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer ./dist/angular-pwa-ngrx-formly-routerstate/stats-es2015.json",
    "transloco:optimize": "transloco-optimize dist/angular-pwa-ngrx-formly-routerstate/assets/i18n",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/cdk": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "~11.0.0",
    "@angular/material": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@angular/service-worker": "~11.0.0",
    "@ngneat/content-loader": "^6.0.0",
    "@ngneat/transloco": "^2.19.1",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngrx/component": "^10.0.1",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/entity": "^10.0.1",
    "@ngrx/router-store": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngx-formly/core": "^5.10.6",
    "@ngx-formly/material": "^5.10.6",
    "bootstrap": "^4.5.3",
    "ngrx-store-localstorage": "^10.1.1",
    "ngx-mat-select-search": "^3.1.4",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1100.1",
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular-devkit/core": "^11.0.1",
    "@angular-devkit/schematics": "^11.0.1",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@componizer/schematics": "0.0.5",
    "@ngneat/transloco-optimize": "^1.0.2",
    "@ngneat/transloco-validator": "^1.0.1",
    "@ngrx/schematics": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "htmlhint": "^0.14.2",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.1",
    "ngrx-tslint-rules": "^2.1.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.5",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "tslint-angular": "^3.0.3",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/app/**/*.ts": [
      "tslint --fix -c tslint.json -p tsconfig.json"
    ],
    "src/app/**/*.{scss,css}": [
      "stylelint --syntax scss --fix"
    ],
    "src/**/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
